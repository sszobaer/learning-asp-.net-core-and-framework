
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
@if (TempData["Msg"] != null)
{
    <p class="alert alert-success">@TempData["Msg"]</p>
}

@if (Model.Count > 0)
 { 

<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Date of Birth</th>
        <th>Actions</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Dob</td>
            <td>
                <a href="">Details</a>
                <a href="Registration/Edit/@item.Id">Edit</a>
                <a href="">Delete</a>
            </td>
        </tr>
    }
</table>
}
else {
<h1>No Data Found</h1>
}
